        <deprecated>false</deprecated>
        <description>Backup action</description>
        <externalId>false</externalId>
        <formula>IF(NOT(InEffect__c), HYPERLINK(&quot;/apex/Apttus_Approval__BackupApproverAction?approverId=&quot; &amp;Current_User__c &amp; &quot;&amp;backupApproverAction=activate&quot;, IMAGE(&quot;/resource/Apttus_Approval__Button_Activate&quot;, &quot;Activate Backup&quot;), &quot;_self&quot;), IF(AND(InEffect__c,IsActive__c), HYPERLINK(&quot;/apex/Apttus_Approval__BackupApproverAction?approverId=&quot; &amp;Current_User__c &amp; &quot;&amp;backupApproverAction=deactivate&quot;, IMAGE(&quot;/resource/Apttus_Approval__Button_Deactivate&quot;, &quot;Deactivate Backup&quot;), &quot;_self&quot;), &quot;&quot;) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Action to activate or deactivate the backup</inlineHelpText>
