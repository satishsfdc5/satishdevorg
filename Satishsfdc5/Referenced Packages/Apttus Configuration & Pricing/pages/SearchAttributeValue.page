			<apex:param name="firstParam" assignTo="{!selectedProductId}" value="" />
		</apex:actionFunction>
		
		<apex:actionFunction name="invokeLoadPage" 
							 action="{!loadPage}"
							 reRender="idSearchResultBlock"
							 status="idPageChangeStatus">
		</apex:actionFunction>
		 
<!-- This is the product details dialog -->
