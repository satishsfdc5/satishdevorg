		        					rendered="false" 
		        					reRender="idErrorMsg, idSearchInputBlock, idSearchResultBlock"
		        					/>

				<apex:commandButton value="{!$Label.apttus_config2__AddMoreProducts}"
									action="{!showProductSelection}"
