    <validationRules>
        <fullName>End_Date_Check</fullName>
        <active>true</active>
        <description>Check that either perpetual is checked or end date is filled in when agreement goes into effect</description>
        <errorConditionFormula>AND(
ISPICKVAL(Status_Category__c,&quot;In Effect&quot;),
AND(ISNULL(Contract_End_Date__c),NOT(Perpetual__c)),
OR(ISNULL(Term_Months__c),Term_Months__c&lt;=0)
)</errorConditionFormula>
        <errorDisplayField>Contract_End_Date__c</errorDisplayField>
        <errorMessage>Please enter the Agreement End Date or mark the Agreement as Perpetual prior to executing the agreement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Term_Validation</fullName>
        <active>true</active>
        <description>Validate that Term is entered correctly</description>
        <errorConditionFormula>AND( ( Perpetual__c ),  (  Term_Months__c  ) &gt; 0 )</errorConditionFormula>
        <errorDisplayField>Term_Months__c</errorDisplayField>
        <errorMessage>Cannot enter both Perpetual and Term.  Please enter only one of the values</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Term_Validation_2</fullName>
        <active>true</active>
        <description>Validate that Term is entered correctly</description>
        <errorConditionFormula>AND( (Perpetual__c ),  NOT(ISNULL(  Contract_End_Date__c )) )</errorConditionFormula>
        <errorDisplayField>Contract_End_Date__c</errorDisplayField>
        <errorMessage>Cannot enter both Perpetual and End Date.  Please enter only one of the values</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Term_Validation_3</fullName>
        <active>false</active>
        <description>Validate that Term is entered correctly (DEPRECATED)</description>
        <errorConditionFormula>AND( (  Term_Months__c ) &gt; 0,  NOT(ISNULL(  Contract_End_Date__c )) )</errorConditionFormula>
        <errorDisplayField>Contract_End_Date__c</errorDisplayField>
        <errorMessage>Cannot enter both Term and End Date.  Please enter only one of the values</errorMessage>
    </validationRules>
